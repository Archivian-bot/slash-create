{
  "name": "slash-create",
  "version": "0.1.0",
  "description": "Creator and handler for Discord's slash commands",
  "main": "./lib/index",
  "author": "Snazzah",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Snazzah/slash-create.git"
  },
  "bugs": {
    "url": "https://github.com/Snazzah/slash-create/issues"
  },
  "keywords": [
    "api",
    "discord",
    "discordapp",
    "slash-command",
    "slash-commands",
    "discord-interactions",
    "wrapper"
  ],
  "scripts": {
    "build": "tsc",
    "changelog": "ts-node scripts/changelog",
    "lint": "npx eslint --ext .ts ./src",
    "lint:fix": "npx eslint --ext .ts ./src --fix",
    "gpr": "ts-node scripts/gpr",
    "prepublishOnly": "npm run eslint:fix && npm run tslint:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build && git add lib"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@discordjs/collection": "^0.1.6",
    "eventemitter3": "^4.0.7",
    "require-all": "^3.0.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.13",
    "@types/require-all": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "fastify": "^3.9.1",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typed-emitter": "^1.3.1",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "express": "^4.17.1",
    "fastify": "^3.9.1"
  }
}
